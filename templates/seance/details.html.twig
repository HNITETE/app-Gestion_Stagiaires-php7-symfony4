{% extends 'base.html.twig' %}
    {% block body %}
        <div class="container col-md-6 "name="" style="margin-left: 250px">
            <h2 class="text-center" style="color: #0a0a0a; font-family: 'Harlow Solid Italic'">Seance DETAILS</h2>
            <h2 class="page-header" style="color: #0a0a0a"> Code Groupe : {{ mdl.codemo }} </h2>
