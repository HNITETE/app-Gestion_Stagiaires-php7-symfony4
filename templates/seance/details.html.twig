            <h2 class="page-header" style="color: #0a0a0a"> heure debut seance : {{ snc.heuredbse | date('h , m') }} </h2>
            <h2 class="page-header" style="color: #0a0a0a"> heure fin seance : {{ snc.heurefnse | date('h , m')}} </h2>
            <h2 class="page-header" style="color: #0a0a0a"> resume seance : {{ snc.resmmese }} </h2>
            <a href="{{ path('seance_liste') }}" class="btn btn-primary btn-block">Back to Stagiaire</a>
        </div>
    {% endblock %}